parserImport XOCL;

import MDC;
import Displays;

context Displays

  @Class InvisibleBox extends Box
  
    @Operation init(values)
      self.showTop := false;
      self.showBottom := false;
      self.showLeft := false;
      self.showRight := false;
      self.fill := false;
      super(values)
    end
    
  end