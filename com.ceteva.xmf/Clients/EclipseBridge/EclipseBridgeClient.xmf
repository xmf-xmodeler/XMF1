parserImport XOCL;

import Clients;
import IO;
import EclipseBridge;

context EclipseBridge

  @Class EclipseBridgeClient extends Client
  
    @Constructor(name)
      self.setCommandInterpreter(EclipseBridgeCommandInterpreter())
    end
    
    @Operation printActions()
      self.commandInterpreter().printActions()
    end
    
  end