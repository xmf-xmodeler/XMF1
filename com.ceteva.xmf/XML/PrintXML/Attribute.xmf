@Class Attribute 
  @Attribute name : Root::XCore::Performable end
  @Attribute value : Root::XCore::Performable end
  @Constructor(name,value) end
  @Operation desugar(out:Performable):Performable
    [| format(<out>," ~S='~X'",Seq{<name>,<value>}) |]
  end
end