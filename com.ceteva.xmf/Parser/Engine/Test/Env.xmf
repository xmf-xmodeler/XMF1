parserImport XOCL;
parserImport Parser::BNF;

import OCL;

context Env::Env

  @Grammar extends O
  
    Env ::= bs = Bs 'end' {
    
      bs->iterate(pair e = [| Env::NullEnv() |] |
        [| <e>.bind(<StrExp(pair->head)>,<pair->tail>) |])
        
    }.
    
    Bs ::= b = B bs = (';' B)* { Seq{b|bs} } | { Seq{} }.
    
    B ::= n = Name '=' e = SimpleExp { Seq{n|e} }.
    
  end