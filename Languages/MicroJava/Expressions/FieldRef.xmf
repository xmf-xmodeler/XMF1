parserImport XOCL;

import Languages;
import MicroJava;
import Expressions;

context Expressions

  @Class FieldRef extends JExp
  
    @Attribute object : JExp    (?) end
    @Attribute name   : String (?) end
    
    @Constructor(object,name) ! end
    
    @Operation pprint(out)
      object.pprint(out);
      format(out,".~S",Seq{name})
    end
    
  end