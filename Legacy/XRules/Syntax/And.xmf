parserImport XOCL;

import XRules;
import Syntax;

context Syntax

  @Class And extends Condition
  
    @Doc
      An and condition is satisfied when objects satisfy the left
      and the right conditions.
    end
    
    @Attribute left  : Condition end
    @Attribute right : Condition end
    
    @Constructor(left,right) end
    
  end